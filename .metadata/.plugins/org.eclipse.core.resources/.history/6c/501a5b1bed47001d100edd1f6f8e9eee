
public class LinkedList{
	// Node, Head
	Node head = null;
	Node tail = null;
	
	static class Node{
		int value;
		Node next;
		
		Node(int d){
			value = d;
			next = null;
		}
	}
	
	public static void main(String[] args) {
		LinkedListDynamic list = new LinkedListDynamic();
		
		list.addNode(1);
		list.addNode(2);
		list.addNode(3);
		
		list.display();
;	}
	
	void addNode(int data) {
		Node newNode = new Node(data);
		
		if(head == null) {
			head = newNode;
			tail = newNode;
		}else {
			tail.next = newNode;
			tail = newNode;
		}
	}
	
	void display() {
		Node current = head;
		
		if(current == null) {
			System.out.println("List is Empty");
		}

		while(current != null) {
			System.out.print(current.value + " ");
			current = current.next;
		}
	}
	
}